def list_match(x)
    match x
        [] => print("Found empty list")
        [5] => print("Found 5")
        10::[] => print("Found 10")
        hd :: tl => print("Using 'hd :: tl' match. Head is: " + to_string(hd))
    end
end

list_match([])
list_match([5])
list_match([10])
list_match([15])
list_match([10,5,1])

print_list([1,2,3,4] ++ [5,6,7,8])
